{
  "name": "georaster",
  "version": "0.2.3",
  "description": "Wrapper around Georeferenced Rasters like GeoTIFF, NetCDF, JPG, and PNG that provides a standard interface",
  "main": "dist/georaster.dist.js",
  "scripts": {
    "babel": "babel src/georaster.js -o dist/georaster.dist.js",
    "browserify": "npm run babel && cd dist && browserify georaster.dist.js -o georaster.browserify.js",
    "build": "npm run browserify",
    "test": "npm run babel && ./node_modules/.bin/mocha --reporter spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GeoTIFF/georaster.git"
  },
  "keywords": [
    "geotiff",
    "gis",
    "maps",
    "raster",
    "rasters",
    "pixels",
    "tiff"
  ],
  "author": "Daniel Dufour and Steve Peyton",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/GeoTIFF/georaster/issues"
  },
  "homepage": "https://github.com/GeoTIFF/georaster#readme",
  "dependencies": {
    "geotiff": "github:danieljdufour/geotiff.js#877f064",
    "path": "^0.12.7",
    "simple-xml-dom": "^1.0.0",
    "underscore": "^1.8.3",
    "webpack": "^3.5.4",
    "xmldom": "^0.1.27",
    "xpath": "0.0.27"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "browserify": "^14.5.0",
    "chai": "^4.1.0",
    "mocha": "^3.5.0"
  }
}
